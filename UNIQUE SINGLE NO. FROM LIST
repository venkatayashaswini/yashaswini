dupList = []
n = int(input("Enter number of items "))
for i in range(1,n+1):
    l= int(input("Enter the %d List Item = " %i))
    dupList.append(l)
print("List Items = ", dupList)
uniqList = []
for i in dupList:
    if i not in uniqList:
        uniqList.append(i)
print("List Items after removing Duplicates = ", uniqList)
