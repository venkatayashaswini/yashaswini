e1 = Entry(cal)
e2 = Entry(cal)
e1.grid(row=0, column=1)
e2.grid(row=1, column=1)
b = Button(cal, text="Addition", command=addNumbers)
b1 = Button(cal, text="sub", command=subNumbers)
b2= Button(cal, text="mul", command=mulNumbers)
b3= Button(cal, text="div", command=divnumbers)
b.grid(row=4,column=1)
b1.grid(row=5,column=1)
b2.grid(row=6,column=1)
b3.grid(row=7,column=1)
